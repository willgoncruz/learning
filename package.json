	"private": true,
	"homepage": "https://willgoncruz.github.io/dummy-project/",
	"dependencies": {
		"@types/react-dom": "^18.0.5",
		"react-dom": "^18.2.0",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-serve": "^2.0.0"
	},
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"format": "prettier --write \"**/*.+(ts|tsx|json|yml|yaml|css|md|vue)\"",
		"format:check": "prettier --check \"**/*.+(ts|tsx|json|yml|yaml|css|md|vue)\"",
