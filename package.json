		"@rollup/plugin-replace": "^4.0.0",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/react": "^18.0.14",
		"babel": "^6.23.0",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.1.0",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"rollup": "^2.75.7",
